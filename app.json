{
  "expo": {
    "name": "application",
    "slug": "application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/guessmate_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/guessmate_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rahmlad.guessmate"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/guessmate_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "73a2043e-343b-4171-93f3-4cbb596921c3"
      }
    }
  }
}
